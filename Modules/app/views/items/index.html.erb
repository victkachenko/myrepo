<h1>Listing items</h1>

<table>
  <thead>
    <tr>
      <th>Day</th>
      <th>Month</th>
      <th>Year</th>
      <th>Rand</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.day %></td>
        <td><%= item.month %></td>
        <td><%= item.year %></td>
        <td><%= item.rand %></td>
        <td><%= link_to 'Show', item %></td>
        <td><%= link_to 'Edit', edit_item_path(item) %></td>
        <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_item_path %>
